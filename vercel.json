{
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://gharconnect.in/:path*",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.gharconnect.in"
        }
      ]
    },
    {
      "source": "/:path*",
      "destination": "https://gharconnect.in/:path*",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "gharconnect.in"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
} 